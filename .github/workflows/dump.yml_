name: dump
on: workflow_dispatch
jobs:
  dumps:
    runs-on: ubuntu-latest
    steps:
      - uses: actions/checkout@v5
      - uses: ./.github/actions/dump/
        id: dump
        with:
          enable-context: 'true'
      - name: echo date
        run: echo ${DATE}
        env:
          DATE: ${{ steps.dump.outputs.dump-date }}